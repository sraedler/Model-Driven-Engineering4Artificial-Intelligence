  author = {Kirchhof, J{\"o}rg Christian and Kusmenko, Evgeny and Ritz, Jonas and Rumpe, Bernhard and Moin, Armin and Badii, Atta and G{\"u}nnemann, Stephan and Challenger, Moharram},
  year = {2022},
