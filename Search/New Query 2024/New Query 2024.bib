@article{hammoudehgarciaBootstrappingMDEDevelopment2021,
  title = {Bootstrapping {{MDE}} Development from {{ROS}} Manual Code: {{Part}} 2---{{Model}} Generation and Leveraging Models at Runtime},
  shorttitle = {Bootstrapping {{MDE}} Development from {{ROS}} Manual Code},
  author = {Hammoudeh~Garc{\'i}a, Nadia and Deshpande, Harshavardhan and Santos, Andr{\'e} and Kahl, Bj{\"o}rn and Bordignon, Mirko},
  year = {2021},
