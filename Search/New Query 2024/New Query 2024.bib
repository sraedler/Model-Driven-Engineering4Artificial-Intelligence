  title = {Trinity: {{A}} No-Code {{AI}} Platform for Complex Spatial Datasets},
  booktitle = {{{GeoAI}}@{{SIGSPATIAL}} 2021: {{Proceedings}} of the 4th {{ACM SIGSPATIAL}} International Workshop on {{AI}} for Geographic Knowledge Discovery, Beijing, China, November 2, 2021},
  author = {Iyer, C. V. Krishnakumar and Hou, Feili and Wang, Henry and Wang, Yonghong and Oh, Kay and Ganguli, Swetava and Pandey, Vipul},
  editor = {Lunga, Dalton D. and Yang, Hsiuhan Lexie and Gao, Song and Martins, Bruno and Hu, Yingjie and Deng, Xueqing and Newsam, Shawn D.},
